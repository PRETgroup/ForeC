		data_remove_GI_reorder_imag_ptr[j+26] = (int)(FFT_output_scale * imag_out_demux_ptr[j+1]);
		data_remove_GI_reorder_real_ptr[j] = (int)(FFT_output_scale * real_out_demux_ptr[j+38]);
		data_remove_GI_reorder_imag_ptr[j] = (int)(FFT_output_scale * imag_out_demux_ptr[j+38]);
