FUNCTION sign_div_unsign_19i (clock, denominator[31..0], numerator[31..0])
RETURNS ( quotient[31..0], remainder[31..0]);

--synthesis_resources = lut 555 reg 1024 
SUBDESIGN lpm_divide_dvs
( 
	clock	:	input;
	denom[31..0]	:	input;
	numer[31..0]	:	input;
	quotient[31..0]	:	output;
	remain[31..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_19i;
	numer_tmp[31..0]	: WIRE;

BEGIN 
	divider.clock = clock;
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
